{
  "version": "1.5.6",
  "strapi": {
    "kind": "plugin",
    "name": "generic-custom-fields",
    "displayName": "Generic Custom Fields",
    "description": "This plugin allows you to easily add custom fields to Strapi."
  },
  "name": "strapi-plugin-generic-custom-fields",
  "description": "This plugin allows you to easily add custom fields to Strapi.",
  "keywords": [
    "strapi",
    "plugin",
    "custom-fields",
    "generic-custom-fields"
  ],
  "author": {
    "name": "Christophe Carvalho Vilas-Boas",
    "url": "https://github.com/ChristopheCVB"
  },
  "maintainers": [
    {
      "name": "Christophe Carvalho Vilas-Boas",
      "url": "https://github.com/ChristopheCVB"
    }
  ],
  "contributors": [
    {
      "name": "Christophe Carvalho Vilas-Boas",
      "url": "https://github.com/ChristopheCVB"
    },
    {
      "name": "Vivian Grimaux",
      "url": "https://github.com/wezerg"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ChristopheCVB/strapi-plugin-generic-custom-fields"
  },
  "license": "MIT",
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "typecheck:front": "tsc --noEmit -p admin/tsconfig.json",
    "typecheck:back": "tsc --noEmit -p server/tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@strapi/design-system": "2.0.0-rc.30",
    "@strapi/icons": "2.0.0-rc.30",
    "@uidotdev/usehooks": "^2.4.1",
    "react-intl": "^7.1.14",
    "slugify": "^1.6.6",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@strapi/sdk-plugin": "^5.4.0",
    "@strapi/strapi": "^5.36.1",
    "@strapi/typescript-utils": "^5.36.1",
    "@stylistic/eslint-plugin": "^5.8.0",
    "@types/koa": "^2.15.0",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "styled-components": "^6.3.11",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.4.0",
    "@strapi/strapi": "^5.36.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "styled-components": "^6.3.11"
  },
  "engines": {
    "node": ">=22.22.0"
  },
  "packageManager": "pnpm@10.30.0",
  "volta": {
    "node": "22.22.0",
    "pnpm": "10.30.0"
  }
}
